{
    "name": "plutuss/laravel-geonames-api",
    "description": "The Laravel Geonames API package provides an easy way to integrate the Geonames API into your Laravel application. This package enables developers to effortlessly interact with the Geonames API to retrieve geographic data such as cities, countries, coordinates, time zones, and other location-based information",
    "keywords": [
        "laravel",
        "GeoNames",
        "geo-names",
        "geonames",
        "api",
        "geolocation",
        "geocoding",
        "geography",
        "location",
        "coordinates",
        "places",
        "cities",
        "countries"
    ],
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "plutuss",
            "email": "dima92konovalenko@gmail.com",
            "homepage": "https://github.com/plutuss"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Plutuss\\GeoNames\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Plutuss\\GeoNames\\Providers\\GeoNamesServiceProvider"
            ]
        }
    }
}
